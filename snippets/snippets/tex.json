{
  "Inline math": {
    "prefix": ["$", "i"],
    "body": [
      "\\$$1\\$$0"
    ],
    "description": "Inline math"
  },
  "Centered math": {
    "prefix": ["$$", "h"],
    "body": [
      "\\[ $1 \\]$0"
    ],
    "description": "Centered math"
  },
  "Section": {
    "prefix": ["sec", "section"],
    "body": [
      "\\section{$1}$0"
    ],
    "description": "Section"
  },
  "Sub Section": {
    "prefix": ["sub", "subsection"],
    "body": [
      "\\subsection{$1}$0"
    ],
    "description": "Subsection"
  },
  "SI Unit": {
    "prefix": "si",
    "body": [
      "\\SI{$1}{$2}$0"
    ],
    "description": "SI Unit"
  },
  "Chemistry formula": {
    "prefix": "ch",
    "body": [
      "\\ch{$1}$0"
    ],
    "description": "Chemical compound"
  },
  "begin": {
    "prefix": "b",
    "body": [
      "\\begin{$1}",
      "  $0",
      "\\end{$1}"
    ],
    "description": "A begin block"
  },
  "Fraction": {
    "prefix": ["/", "frac"],
    "body": [
      "\\frac{$1}{$2}$0"
    ],
    "description": "A fraction"
  },
  "Cdot": {
    "prefix": ["*", "c", "cdot"],
    "body": [
      "\\cdot $0"
    ],
    "description": "A cdot"
  },
  "Parenthesis": {
    "prefix": ["(", "paren"],
    "body": [
      "\\left($1\\right)$0"
    ],
    "description": "Variable height parenthesis"
  },
  "Absolute Value": {
    "prefix": ["|", "abs"],
    "body": [
      "\\|$1\\|$0"
    ],
    "description": "An absolute value block"
  },
  "Text": {
    "prefix": ["text", "txt"],
    "body": [
      "\\text{$1}$0"
    ],
    "description": "Text block for inside math"
  },
  "Bold": {
    "prefix": ["bf", "bold", "textbf"],
    "body": [
      "\\textbf{$1}$0"
    ],
    "description": "Bold text"
  },
  "Italics": {
    "prefix": ["it", "italic", "textit"],
    "body": [
      "\\textit{$1}$0"
    ],
    "description": "Italic text"
  }
}
